<widget
    xmlns:tizen="http://tizen.org/ns/widgets"
    xmlns="http://www.w3.org/ns/widgets"
    id="http://com.samsung.mpl/GearInputConsumer"
    version="1.0.5"
    viewmodes="maximized">
    <tizen:app-control>
        <tizen:src name="index.html" />
        <tizen:operation
            name="http://tizen.org/appcontrol/operation/remote_settings" />
